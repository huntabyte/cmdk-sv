{
	"name": "cmdk-sv",
	"version": "0.0.18",
	"scripts": {
		"build": "pnpm package",
		"dev": "pnpm watch",
		"dev:svelte": "vite dev",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"test": "vitest",
		"watch": "svelte-package --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next.1"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"publint": "^0.2.8",
		"svelte": "^5.0.0-next.257",
		"svelte-check": "^3.8.4",
		"typescript": "^5.6.2",
		"vite": "^5.4.7",
		"vitest": "^2.0.2"
	},
	"dependencies": {
		"bits-ui": "https://pkg.pr.new/huntabyte/bits-ui/bits-ui@f348dab",
		"runed": "^0.15.2",
		"svelte-toolbelt": "^0.3.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"engines": {
		"pnpm": ">=8.7.0",
		"node": ">=18"
	},
	"packageManager": "pnpm@9.6.0"
}
